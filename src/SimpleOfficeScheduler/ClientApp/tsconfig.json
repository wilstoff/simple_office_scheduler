{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../wwwroot/js",
    "sourceMap": true,
    "declaration": false
  },
  "include": ["src/**/*.ts"]
}
